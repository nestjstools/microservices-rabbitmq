{
  "name": "@nestjstools/microservices-rabbitmq",
  "version": "1.0.0",
  "description": "Transport layer for NestJS Microservices with more configurations for RabbitMQ",
  "author": "Sebastian Iwanczyszyn",
  "private": false,
  "license": "MIT",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/nestjstools/microservices-rabbitmq/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nestjstools/microservices-rabbitmq"
  },
  "keywords": [
    "nestjs-tools",
    "nestjs",
    "nestjs-microservices",
    "rabbitmq",
    "rabbitmq-messages",
    "nestjs-rabbitmq",
    "nestjs-microservices-rabbitmq"
  ],
  "files": [
    "lib",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "nest build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "rabbitmq-client": "^5.0.2"
  },
  "peerDependencies": {
    "@nestjs/microservices": "^11.X || ^10.X"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/microservices": "^11.0.10",
    "@nestjs/platform-express": "^11.0.1",
    "amqp-client": "^0.1.2",
    "rabbitmq-client": "^5.0.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
